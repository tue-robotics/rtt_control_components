import("rtt_ros")
ros.import("rtt_control_components")

loadComponent("FollowJointTrajectoryActionToDoubles","ROS::FollowJointTrajectoryActionToDoubles")
addPeer("Deployer","FollowJointTrajectoryActionToDoubles")

loadService("FollowJointTrajectoryActionToDoubles","actionlib")

FollowJointTrajectoryActionToDoubles.actionlib.connect("/joint_trajectory_action_left");

setActivity("FollowJointTrajectoryActionToDoubles",0.1,LowestPriority,ORO_SCHED_OTHER)

FollowJointTrajectoryActionToDoubles.configure

FollowJointTrajectoryActionToDoubles.start


